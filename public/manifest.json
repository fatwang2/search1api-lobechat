{
  "$schema": "../node_modules/@lobehub/chat-plugin-sdk/schema.json",
  "api": [
    {
      "name": "search1api",
      "url": "https://lobehub.search1api.com/api/search",
      "description": "又快又便宜的搜索聚合服务",
      "parameters": {
        "properties": {
          "query": {
            "description": "搜索的文本内容",
            "type": "string"
          }
        },
        "required": ["query"],
        "type": "object"
      }
    }
  ],
  "author": "fatwang2",
  "homepage": "https://github.com/fatwang2/search1api-lobechat",
  "identifier": "search1api",
  "meta": {
    "avatar": "https://cdn.discordapp.com/attachments/1109354586604585011/1236568039198167050/favicon.webp?ex=66387b3a&is=663729ba&hm=38435c611569cf1895bd89a0181c0cb09de2f5d659a4e913c4a51c5ca22e7131&",
    "tags": ["web", "search"],
    "title": "Search1API",
    "description": "搜索聚合服务，专为大模型优化"
  },
  "settings": {
    "type": "object",
    "required": ["SEARCH1API_KEY"],
    "properties": {
      "SEARCH1API_KEY": {
        "title": "Search1API API Key",
        "description": "we use [Search1API](https://search1api.com) as backend service | 该插件使用 Search1API 作为搜索服务",
        "type": "string",
        "format": "password"
      }
    }
  },
  "version": "1"
}
